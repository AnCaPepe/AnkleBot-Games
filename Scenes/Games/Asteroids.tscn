[sub_resource type="GDScript" id=2]

script/source = "extends KinematicBody

onready var max_position = get_parent().shape.extents.y

func _physics_process( delta ):
	var controller_values = Controller.get_axis_values( Controller.VERTICAL )
	var new_position = controller_values[ Controller.POSITION ] * max_position
	new_position = clamp( new_position, -max_position, max_position )
	var position_delta = new_position - translation.y
	translation.y = new_position
	rotation.x = ( rotation.x + 0.15 * position_delta / delta ) / 2
	
	DataLog.register_values( [ translation.y ] )"

