script/source = "extends Panel

onready var server_address = get_node( \"AddressInput\" ).text
onready var user_name = get_node( \"UserInput\" ).text
onready var position_slider = get_node( \"PositionSlider\" )
onready var position_display = get_node( \"PositionSlider/NumericDisplay\" )

func _ready():
	Controller.set_status( 1 )

func _input( event ):
	if event is InputEventKey:
		if event.scancode == KEY_ESCAPE: OS.set_window_fullscreen( false )

func _process( delta ):
	var axis_values = Controller.get_axis_values( Controller.VERTICAL )
	var position = axis_values[ Controller.POSITION ]
	position_slider.value = position
	position_display.text = ( \"%+.3f\" % position )

func _on_ConnectButton_pressed():
	Controller.connect_client( server_address, 8000 )
	Controller.set_user( user_name )
 
func _on_AddressInput_text_entered( new_text ):
	server_address = new_text

func _on_UserInput_text_entered( new_text ):
	user_name = new_text

func _on_SetpointSlider_value_changed( value ):
	Controller.set_axis_values( Controller.VERTICAL, value, 1 )
"

[sub_resource type="GDScript" id=3]

script/source = "extends MenuButton

const GAMES_PATH = \"res://Scenes/Games\"
const GAME_EXT = \".tscn\"

func _ready():
	get_popup().add_font_override( \"font\", get_font( \"font\" ) )
	var games_dir = Directory.new()
	games_dir.open( GAMES_PATH )
	games_dir.list_dir_begin()
	while true:
		var file = games_dir.get_next()
		if file == \"\": break
		elif file.ends_with( GAME_EXT ):
			get_popup().add_item( file.get_basename() )
	games_dir.list_dir_end()
	
	get_popup().connect( \"index_pressed\", self, \"_on_index_pressed\" )

func _on_index_pressed( index ):
	var game_name = get_popup().get_item_text( index )
	get_tree().change_scene( GAMES_PATH + \"/\" + game_name + GAME_EXT )
"

